{
    "name": "Last.fm",
    "apiVersion": "2.04",
    "baseUrl": "http://ws.audioscrobbler.com/2.0",
    "description": "Last.fm/Audioscrobbler API",
    "operations": {
        "AbstractUnauthenticated": {
            "httpMethod": "GET",
            "parameters": {
                "api_key": {
                    "location": "query",
                    "type": "string"
                },
                "method": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "format": {
                    "location": "query",
                    "type": "string"
                }
            }
        },
        "AbstractArtist": {
            "extends": "AbstractUnauthenticated",
            "httpMethod": "GET",
            "parameters": {
                    "artist": {
                    "location": "query",
                    "type": "string",
                    "description": "The artist name."
                },
                "autocorrect": {
                    "location": "query",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 1,
                    "description": "Transform misspelled artist names into correct artist names, returning the correct version instead. The corrected artist name will be returned in the response."
                },
                "mbid": {
                    "location": "query",
                    "type": "string",
                    "description": "The musicbrainz id for the album."
                }
            }
        },
        "getArtistInfo": {
            "extends": "AbstractArtist",
            "parameters": {
                "method": {
                    "location": "query",
                    "type": "string",
                    "default": "artist.getInfo",
                    "static": true
                },
                "lang": {
                    "location": "query",
                    "type": "string",
                    "description": "The language to return the biography in, expressed as an ISO 639 alpha-2 code."
                },
                "username": {
                    "location": "query",
                    "type": "string",
                    "description": "The username for the context of the request. If supplied, the user's playcount for this artist is included in the response."
                }
            }
        }
    }
}
